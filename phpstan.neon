includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:
    paths:
        - src
    level: 5
    excludePaths:
        - src/Auth/Manager.php
        - src/Assetic/*
    databaseMigrationsPath:
        - src/Auth/Migrations
        - src/Database/Migrations
    ignoreErrors:
        - message: '#calls parent::__#'
          path: src/Extension/ExtendableTrait.php
